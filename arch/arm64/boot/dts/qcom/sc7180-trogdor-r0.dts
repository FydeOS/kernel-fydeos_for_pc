/dts-v1/;

#include "sc7180.dtsi"

ap_ec_spi: &spi0 {};
ap_h1_spi: &spi6 {};

#include "sc7180-trogdor.dtsi"

/ {
	model = "Google Trogdor (rev0)";
	compatible = "google,trogdor-rev0", "qcom,sc7180";
};

&ap_pen {
	/* Not hooked up */
	status = "disabled";
};

&ap_sar_sensor_i2c {
	/* Not hooked up */
	status = "disabled";
};

&ap_spi_fp {
	/* Not hooked up */
	status = "disabled";
};

&ap_tp_i2c {
	/* Pullups are missing on r0, but it seems to work at 100 kHz */
	clock-frequency = <100000>;
};

/delete-node/ &brij_pp1200_en;

&cr50 {
	interrupts = <21 IRQ_TYPE_EDGE_RISING>;
};

/delete-node/ &pp1200_brij;
pp1200_brij: &pp1200_l3c {};

&sn65dsi86_bridge {
	vcca-supply = <&pp1200_brij>;
	vcc-supply = <&pp1200_brij>;
};

/* PINCTRL - modifications to sc7180-trogdor.dtsi */

&ap_suspend_l_neuter {
	pinmux  {
		pins = "gpio20";
	};

	pinconf {
		pins = "gpio20";
	};
};

&edp_brij_en {
	pinmux {
		pins = "gpio14";
	};

	pinconf {
		pins = "gpio14";
	};
};

&h1_ap_int_odl {
	pinmux {
		pins = "gpio21";
	};

	pinconf {
		pins = "gpio21";
	};
};

&pen_irq_l {
	pinmux {
		pins = "gpio104";
	};

	pinconf {
		pins = "gpio104";
	};
};

